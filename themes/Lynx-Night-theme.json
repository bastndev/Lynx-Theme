{
  "$schema": "vscode://schemas/color-theme",
  "semanticClass": "lynx-night-theme",
  "semanticHighlighting": true,
  "name": "Lynx Night Theme",
  "type": "dark",

  "colors": {
    // TODO: Menu Status Bar MARK: - [TOP]
    "titleBar.activeBackground":             "#192124",    // Background
    "titleBar.activeForeground":             "#c9d1d9",    // TOP text 1
    "titleBar.border":                       "#192124",    // LINE
    "titleBar.inactiveBackground":           "#192124",    // Inactive window title
    "titleBar.inactiveForeground":           "#6b7280",    // Inactive title text

    // --- -- - Menu Click & select  
    "menu.background":                       "#192124",    // Menu background
    "menu.foreground":                       "#c9d1d9",    // Menu text
    "menu.selectionBackground":              "#2d3540",    // Selected item background
    "menu.selectionForeground":              "#c9d1d9",    // Active click color
    "menu.separatorBackground":              "#2a2e33",    // Separator line
    "menubar.selectionBackground":           "#2d3540",    // Menubar selection background
    "menubar.selectionForeground":           "#c9d1d9",    // Hover TOP text 1

    "list.hoverBackground":                  "#222a30",    // Hover background
    "list.hoverForeground":                  "#c9d1d9",    // Text select
    "list.dropBackground":                   "#4ECDC430",  // Drop background
    
    // TODO: Activity Bar MARK: [ONE-1]
    "activityBar.background":                "#192124",    // Activity bar background
    "activityBar.foreground":                "#c9d1d9",    // Activity bar text
    "activityBar.border":                    "#192124",    // Activity bar border
    "activityBar.inactiveForeground":        "#6b7280",    // No selection icon
    "activityBarBadge.background":           "#5a6b5a",    // Badge background
    "activityBarBadge.foreground":           "#ffffff",    // Badge text
    
    // --- -- - Menu hover - click & select = RIGHT
    "list.activeSelectionBackground":        "#3a4a55",    // Active background
    "list.activeSelectionForeground":        "#c9d1d9",    // Active text color
    "list.inactiveSelectionForeground":      "#c9d1d9",    // Inactive text color
    "list.inactiveSelectionBackground":      "#252e38",    // Inactive background
    "list.focusBackground":                  "#3a4a55",    // Focus background
    "list.focusForeground":                  "#c9d1d9",    // Focus text
    "list.highlightForeground":              "#4ECDC4",    // Highlight text color

    // --- -- - Side Bar
    "sideBar.background":                    "#192124",    // Side bar background
    "sideBar.foreground":                    "#c9d1d9",    // Text vscode all
    "sideBar.border":                        "#192124",    // LINE RIGHT & LEFT
    "sideBarSectionHeader.background":       "#192124",    // LINE bg
    "sideBarSectionHeader.border":           "#192124",    // Line top
    "sideBarTitle.foreground":               "#c9d1d9",    // Explorer
    "sideBarSectionHeader.foreground":       "#4ECDC4",    // Css text and BOT txt

    // --- -- - Buttons
    "button.background":                     "#9B59B6",    // Button background
    "button.foreground":                     "#ffffff",    // Button text
    "button.hoverBackground":                "#8e4ec6",    // Button hover
    "button.border":                         "#9B59B6",    // Button border

    // --- -- - Editor hover widget
    "editorHoverWidget.background":          "#192124",    // Hover widget background
    "editorHoverWidget.foreground":          "#c9d1d9",    // Hover widget text
    "editorHoverWidget.border":              "#2a2e33",    // Hover widget border

    //--- -- - GIT Decoration
    "gitDecoration.modifiedResourceForeground":      "#F39C12",    // Modified files
    "gitDecoration.conflictingResourceForeground":   "#c78a7a",    // Merge conflicts
    "gitDecoration.deletedResourceForeground":       "#c78a7a",    // Deleted files
    "gitDecoration.ignoredResourceForeground":       "#4a5568",    // Ignored files
    "gitDecoration.untrackedResourceForeground":     "#8FBC8F",    // Untracked files
    "gitDecoration.stageModifiedResourceForeground": "#F39C12",    // Staged modified files
    "gitDecoration.stageDeletedResourceForeground":  "#c78a7a",    // Staged deleted files
    "gitDecoration.submoduleResourceForeground":     "#4ECDC4",    // Submodules

    // TODO: EDITOR CODE MARK: [TWO-2]
    "editor.background":                     "#192124",    // Editor background
    "editor.foreground":                     "#c9d1d9",    // Editor text
    "editor.findMatchBackground":            "#9B59B630",  // Find match background
    "editor.findMatchHighlightBackground":   "#9B59B620",  // Find match highlight background
    "editor.selectionBackground":            "#3a4a55",    // Selection background
    "editor.selectionHighlightBackground":   "#3a4a55",    // Selection highlight background
    "editor.inactiveSelectionBackground":    "#252e38",    // Inactive selection background
    "editor.wordHighlightBackground":        "#4ECDC420",  // Word highlight background
    "editor.wordHighlightStrongBackground":  "#4ECDC430",  // Word highlight strong background

    // --- -- - Tabs Header - TOP
    "editorGroupHeader.tabsBackground":      "#192124",    // Tabs header background
    "editorGroupHeader.tabsBorder":          "#192124",    // Tabs header border
    "tab.activeBackground":                  "#192124",    // Active tab background
    "tab.activeForeground":                  "#c9d1d9",    // Active tab text
    "tab.activeBorder":                      "#192124",    // Active tab border
    "tab.activeBorderTop":                   "#8FBC8F",    // Active tab top border
    "tab.inactiveBackground":                "#192124",    // Inactive tab background
    "tab.inactiveForeground":                "#6b7280",    // Inactive tab text
    "tab.border":                            "#192124",    // Tab border
    "tab.hoverBackground":                   "#192124",    // Tab hover background
    "tab.hoverForeground":                   "#c9d1d9",    // Tab hover text
    "tab.hoverBorder":                       "#192124",    // Tab hover border
    "tab.unfocusedActiveForeground":         "#8fa1b3",    // Unfocused active tab text
    "tab.unfocusedInactiveForeground":       "#4a5568",    // Unfocused inactive tab text

    // --- -- - Breadcrumb second-Header
    "breadcrumb.background":                 "#192124",    // Breadcrumb background
    "breadcrumb.foreground":                 "#6b7280",    // Breadcrumb text
    "breadcrumbPicker.background":           "#192124",    // Breadcrumb picker background
    "breadcrumb.focusForeground":            "#c9d1d9",    // Breadcrumb focus text
    "breadcrumb.activeSelectionForeground":  "#4ECDC4",    // Breadcrumb active selection text

    // --- -- - Scrollbar - RIGHT
    "scrollbarSlider.background":            "#2a354040",  // Scroll bar small and large
    "scrollbarSlider.hoverBackground":       "#2a354060",  // Hover background
    "scrollbarSlider.activeBackground":      "#2a354080",  // Active background
    "scrollbar.shadow":                      "#00000050",  // Shadow

    // --- -- - Editor Line Number - LEFT
    "editorLineNumber.activeForeground":     "#6b8fa3",    // Active line number
    "editorLineNumber.foreground":           "#4a5568",    // Line numbers

    // --- -- - Editor Suggest widget MID
    "editorSuggestWidget.background":        "#192124",    // Suggestion widget background
    "editorSuggestWidget.foreground":        "#c9d1d9",    // Suggestion widget text
    "editorSuggestWidget.border":            "#2a2e33",    // Suggestion widget border

    // --- -- - Editor Widgets - MID
    "editorWidget.background":               "#192124",    // Widget background
    "editorWidget.border":                   "#2a2e33",    // Widget border
    "widget.border":                         "#2a2e33",    // General widget border
    "widget.shadow":                         "#00000050",  // Widget shadow

    // TODO: Input Controls MARK: [THREE-3]
    "input.background":                      "#192124",    // Input background
    "input.foreground":                      "#c9d1d9",    // Input text color
    "input.border":                          "#2a2e33",    // Input border color
    "input.placeholderForeground":           "#6b7280",    // Input placeholder text
    "focusBorder":                           "#6b8fa3",    // Focus border
    "editorCursor.foreground":               "#6b8fa3",    // Cursor color

    // --- -- - progress bar
    "progressBar.background":                "#9B59B6",    // Progress bar background
    
    // --- -- - Panel Terminal & Text BOTTOM
    "panel.background":                      "#192124",    // Panel background
    "panel.foreground":                      "#c9d1d9",    // Panel text
    "panel.border":                          "#192124",    // Panel border
    "panelTitle.activeBorder":               "#8FBC8F",    // Active panel title border
    "panelTitle.activeForeground":           "#c9d1d9",    // Active panel title text
    "panelTitle.inactiveForeground":         "#6b7280",    // Inactive panel title text
    "panelSection.border":                   "#2a2e33",    // Panel section border
    "panelSection.dropBackground":           "#4ECDC430",  // Panel section drop background
    "panelSectionHeader.background":         "#192124",    // Panel section header background
    "panelSectionHeader.foreground":         "#4ECDC4",    // Panel section header text

    // --- -- - Terminal colors
    "terminal.background":                   "#192124",    // Terminal background
    "terminal.foreground":                   "#c9d1d9",    // Terminal text
    "terminal.ansiBlack":                    "#192124",    // Terminal black color
    "terminal.ansiBlue":                     "#4ECDC4",    // Terminal blue color
    "terminal.ansiBrightBlack":              "#4a5568",    // Terminal bright black color
    "terminal.ansiBrightBlue":               "#66D9EF",    // Terminal bright blue color
    "terminal.ansiBrightCyan":               "#4ECDC4",    // Terminal bright cyan color
    "terminal.ansiBrightGreen":              "#8FBC8F",    // Terminal bright green color
    "terminal.ansiBrightMagenta":            "#9B59B6",    // Terminal bright magenta color
    "terminal.ansiBrightRed":                "#E91E63",    // Terminal bright red color
    "terminal.ansiBrightWhite":              "#ffffff",    // Terminal bright white color
    "terminal.ansiBrightYellow":             "#F39C12",    // Terminal bright yellow color
    "terminal.ansiCyan":                     "#4ECDC4",    // Terminal cyan color
    "terminal.ansiGreen":                    "#8FBC8F",    // Terminal green color
    "terminal.ansiMagenta":                  "#9B59B6",    // Terminal magenta color
    "terminal.ansiRed":                      "#E91E63",    // Terminal red color
    "terminal.ansiWhite":                    "#c9d1d9",    // Terminal white color
    "terminal.ansiYellow":                   "#F39C12",    // Terminal yellow color
    "terminal.selectionBackground":          "#2d3540",    // Terminal selection background

    // --- -- - Notifications
    "notifications.background":              "#192124",    // Notification background
    "notifications.foreground":              "#c9d1d9",    // Notification text
    "notifications.border":                  "#2a2e33",    // Notification border
    "notificationCenter.border":             "#2a2e33",    // Notification center border
    "notificationCenterHeader.background":   "#192124",    // Notification center header background
    "notificationCenterHeader.foreground":   "#4ECDC4",    // Notification center header text
    "notificationToast.border":              "#2a2e33",    // Notification toast border
    "notificationsErrorIcon.foreground":     "#c78a7a",    // Error notification icon
    "notificationsWarningIcon.foreground":   "#F39C12",    // Warning notification icon
    "notificationsInfoIcon.foreground":      "#4ECDC4",    // Info notification icon

    // --- -- - Dropdown Controls
    "dropdown.background":                   "#192124",    // Dropdown background
    "dropdown.foreground":                   "#c9d1d9",    // Dropdown text color
    "dropdown.border":                       "#2a2e33",    // Dropdown border
    "dropdown.listBackground":               "#192124",    // Dropdown list background

    // TODO:Status Bar MARK: - [BOTTOM]
    "statusBar.background":                  "#192124",    // Status bar background
    "statusBar.foreground":                  "#c9d1d9",    // Status bar text color
    "statusBar.border":                      "#192124",    // Status bar border
    "statusBar.debuggingBackground":         "#F39C12",    // Status bar debugging background
    "statusBar.debuggingForeground":         "#192124",    // Status bar debugging text
    "statusBar.noFolderBackground":          "#192124",    // Status bar no folder background
    "statusBar.noFolderForeground":          "#c9d1d9",    // Status bar no folder text
    "statusBarItem.hoverBackground":         "#3a4a55",    // Status bar item hover background
    "statusBarItem.activeBackground":        "#3a4a55",    // Status bar item active background
    "statusBarItem.remoteForeground":        "#4ECDC4",    // Remote status text
    "statusBarItem.remoteBackground":        "#192124",    // Remote status background

    // --- -- - BG {} and () and []
    "editorBracketHighlight.foreground1":    "#8FBC8F",    // Bracket highlight color 1
    "editorBracketHighlight.foreground2":    "#4ECDC4",    // Bracket highlight color 2
    "editorBracketHighlight.foreground3":    "#c78a7a",    // Bracket highlight color 3
    "editorBracketHighlight.foreground4":    "#F39C12",    // Bracket highlight color 4
    "editorBracketHighlight.foreground5":    "#66D9EF",    // Bracket highlight color 5
    "editorBracketHighlight.foreground6":    "#FFB74D",    // Bracket highlight color 6
    "editorBracketHighlight.unexpectedBracket.foreground": "#c78a7a", // Unexpected bracket color
    "editorBracketMatch.background":         "#3a4a5540",  // Bracket match background
    "editorBracketMatch.border":             "#6b8fa3",    // Bracket match border

    // ------------------------------------------
    // Código adicional no presente en Lynx-Dark-theme.json
    
    // --- -- - Editor additional settings
    "editorIndentGuide.background":          "#2a2e33",    // Guías de indentación
    "editorIndentGuide.activeBackground":    "#4ECDC4",    // Guía de indentación activa
    "editorRuler.foreground":                "#2a2e33",    // Líneas de regla del editor
    "editorWhitespace.foreground":           "#4a5568",    // Espacios en blanco
    "editorError.foreground":                "#c78a7a",    // Errores del editor
    "editorWarning.foreground":              "#F39C12",    // Advertencias del editor
    "editorInfo.foreground":                 "#4ECDC4",    // Información del editor
    "editorHint.foreground":                 "#8FBC8F",    // Sugerencias del editor

    // --- -- - Input validation
    "inputOption.activeBorder":              "#9B59B6",    // Borde de opción de entrada activa
    "inputOption.activeBackground":          "#9B59B620",  // Fondo de opción de entrada activa
    "inputOption.activeForeground":          "#9B59B6",    // Texto de opción de entrada activa
    "inputValidation.errorBackground":       "#E91E6320",  // Fondo de error de validación
    "inputValidation.errorForeground":       "#c78a7a",    // Texto de error de validación
    "inputValidation.errorBorder":           "#c78a7a",    // Borde de error de validación
    "inputValidation.warningBackground":     "#F39C1220",  // Fondo de advertencia de validación
    "inputValidation.warningForeground":     "#F39C12",    // Texto de advertencia de validación
    "inputValidation.warningBorder":         "#F39C12",    // Borde de advertencia de validación
    "inputValidation.infoBackground":        "#4ECDC420",  // Fondo de información de validación
    "inputValidation.infoForeground":        "#4ECDC4",    // Texto de información de validación
    "inputValidation.infoBorder":            "#4ECDC4",    // Borde de información de validación

    // --- -- - Peek view
    "peekView.background":                   "#192124",    // Fondo de vista previa
    "peekViewEditor.background":             "#192124",    // Fondo del editor de vista previa
    "peekViewEditor.matchHighlightBackground": "#9B59B650", // Resaltado de coincidencias en vista previa
    "peekViewResult.background":             "#192124",    // Fondo de resultados de vista previa
    "peekViewResult.fileForeground":         "#c9d1d9",    // Texto de archivo en vista previa
    "peekViewResult.lineForeground":         "#6b7280",    // Texto de línea en vista previa
    "peekViewResult.matchHighlightBackground": "#9B59B630", // Resaltado de coincidencias en resultados
    "peekViewResult.selectionBackground":    "#2d3540",    // Fondo de selección en resultados
    "peekViewResult.selectionForeground":    "#c9d1d9",    // Texto de selección en resultados
    "peekViewTitle.background":              "#192124",    // Fondo del título de vista previa
    "peekViewTitleDescription.foreground":   "#6b7280",    // Descripción del título de vista previa
    "peekViewTitleLabel.foreground":         "#4ECDC4",    // Etiqueta del título de vista previa

    // --- -- - Merge conflicts
    "merge.currentHeaderBackground":         "#4ECDC440",  // Fondo del encabezado actual en merge
    "merge.currentContentBackground":        "#4ECDC420",  // Fondo del contenido actual en merge
    "merge.incomingHeaderBackground":        "#9B59B640",  // Fondo del encabezado entrante en merge
    "merge.incomingContentBackground":       "#9B59B620",  // Fondo del contenido entrante en merge
    "merge.border":                          "#2a2e33",    // Borde de merge
    "merge.commonContentBackground":         "#8FBC8F20",  // Fondo del contenido común en merge
    "merge.commonHeaderBackground":          "#8FBC8F40",  // Fondo del encabezado común en merge

    // --- -- - Overview ruler
    "editorOverviewRuler.border":            "#2a2e33",    // Borde de la regla de vista general
    "editorOverviewRuler.findMatchForeground": "#9B59B6",  // Coincidencias de búsqueda en regla
    "editorOverviewRuler.rangeHighlightForeground": "#4ECDC4", // Resaltado de rango en regla
    "editorOverviewRuler.selectionHighlightForeground": "#9B59B640", // Resaltado de selección en regla
    "editorOverviewRuler.wordHighlightForeground": "#4ECDC440", // Resaltado de palabra en regla
    "editorOverviewRuler.wordHighlightStrongForeground": "#4ECDC460", // Resaltado fuerte de palabra en regla
    "editorOverviewRuler.modifiedForeground": "#F39C12",   // Líneas modificadas en regla
    "editorOverviewRuler.addedForeground":   "#8FBC8F",    // Líneas añadidas en regla
    "editorOverviewRuler.deletedForeground": "#c78a7a",    // Líneas eliminadas en regla
    "editorOverviewRuler.errorForeground":   "#c78a7a",    // Errores en regla
    "editorOverviewRuler.warningForeground": "#F39C12",    // Advertencias en regla
    "editorOverviewRuler.infoForeground":    "#4ECDC4",    // Información en regla
    "editorOverviewRuler.currentContentForeground": "#4ECDC4", // Contenido actual en regla
    "editorOverviewRuler.incomingContentForeground": "#9B59B6", // Contenido entrante en regla
    "editorOverviewRuler.commonContentForeground": "#8FBC8F", // Contenido común en regla

    // --- -- - Git decoration additional
    "gitDecoration.renamedResourceForeground": "#F39C12",  // Archivos renombrados
    "gitDecoration.addedResourceForeground": "#8FBC8F",    // Archivos añadidos

    // --- -- - Settings
    "settings.headerForeground":             "#c9d1d9",    // Encabezado de configuración
    "settings.modifiedItemIndicator":        "#4ECDC4",    // Indicador de elemento modificado
    "settings.dropdownBackground":           "#192124",    // Fondo de dropdown en configuración
    "settings.dropdownForeground":           "#c9d1d9",    // Texto de dropdown en configuración
    "settings.dropdownBorder":               "#2a2e33",    // Borde de dropdown en configuración
    "settings.checkboxBackground":           "#192124",    // Fondo de checkbox en configuración
    "settings.checkboxForeground":           "#4ECDC4",    // Texto de checkbox en configuración
    "settings.checkboxBorder":               "#2a2e33",    // Borde de checkbox en configuración
    "settings.textInputBackground":          "#192124",    // Fondo de entrada de texto en configuración
    "settings.textInputForeground":          "#c9d1d9",    // Texto de entrada en configuración
    "settings.textInputBorder":              "#2a2e33",    // Borde de entrada de texto en configuración
    "settings.numberInputBackground":        "#192124",    // Fondo de entrada numérica en configuración
    "settings.numberInputForeground":        "#c9d1d9",    // Texto de entrada numérica en configuración
    "settings.numberInputBorder":            "#2a2e33",    // Borde de entrada numérica en configuración

    // --- -- - Extensions
    "extensionButton.prominentBackground":   "#9B59B6",    // Fondo prominente de botón de extensión
    "extensionButton.prominentForeground":   "#ffffff",    // Texto prominente de botón de extensión
    "extensionButton.prominentHoverBackground": "#8e4ec6", // Hover prominente de botón de extensión

    // --- -- - Diff editor
    "diffEditor.insertedTextBackground":     "#8FBC8F20",  // Fondo de texto insertado en diff
    "diffEditor.removedTextBackground":      "#c78a7a20",  // Fondo de texto eliminado en diff
    "diffEditor.insertedLineBackground":     "#8FBC8F10",  // Fondo de línea insertada en diff
    "diffEditor.removedLineBackground":      "#c78a7a10",  // Fondo de línea eliminada en diff

    // --- -- - Additional elements
    "editorGroup.border":                    "#192124",    // Borde de grupo de editor
    "editorGroupHeader.border":              "#192124",    // Borde de encabezado de grupo de editor
    "editorPane.background":                 "#192124",    // Fondo de panel de editor
    "window.activeBorder":                   "#192124",    // Borde de ventana activa
    "window.inactiveBorder":                 "#192124",    // Borde de ventana inactiva
    "contrastBorder":                        "#192124",    // Borde de contraste
    "selection.background":                  "#3a4a5540",  // Fondo de selección general
    "toolbar.hoverBackground":               "#2d3540",    // Fondo de hover en toolbar
    "toolbar.activeBackground":              "#2d3540",    // Fondo activo en toolbar
    "quickInput.background":                 "#192124",    // Fondo de entrada rápida
    "quickInput.foreground":                 "#c9d1d9",    // Texto de entrada rápida
    "quickInputTitle.background":            "#192124"     // Fondo de título de entrada rápida
  },
  "tokenColors": [
    // ------------------------------------------
    // Código adicional de tokenColors no presente en Lynx-Dark-theme.json
    
    // --- -- - Comentarios adicionales
    {
      "name": "Comment Keywords",
      "scope": [
        "comment.line.double-slash keyword",
        "comment.block keyword"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#6b7280"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSDoc Tags",
      "scope": [
        "storage.type.class.jsdoc",
        "entity.name.type.instance.jsdoc"
      ],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },

    // --- -- - Variables adicionales
    {
      "name": "Variable Destructuring",
      "scope": [
        "variable.other.destructuring"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },

    // --- -- - Propiedades y claves de objeto
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.property.js",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },

    // --- -- - Keywords adicionales
    {
      "name": "Control Flow Keywords",
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import/Export Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "italic"
      }
    },

    // --- -- - Tipos adicionales
    {
      "name": "Type Parameters",
      "scope": [
        "entity.name.type.parameter",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#c78a7a",
        "fontStyle": "italic"
      }
    },

    // --- -- - Strings adicionales
    {
      "name": "Template Strings",
      "scope": [
        "string.template",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "String Escape Characters",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "bold"
      }
    },

    // --- -- - Operadores especiales
    {
      "name": "Special Operators",
      "scope": [
        "keyword.operator.ternary",
        "keyword.operator.spread",
        "keyword.operator.rest"
      ],
      "settings": {
        "foreground": "#F39C12"
