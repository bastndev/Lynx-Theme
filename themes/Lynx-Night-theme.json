    "terminal.ansiBrightBlue":               "#62cdff",    // Terminal bright blue color
    "terminal.ansiBrightCyan":               "#00d0e7",    // Terminal bright cyan color
    "terminal.ansiBrightGreen":              "#0dff00",    // Terminal bright green color
    "terminal.ansiBrightMagenta":            "#c792ea",    // Terminal bright magenta color
    "terminal.ansiBrightRed":                "#ed0a3f",    // Terminal bright red color
    "terminal.ansiBrightWhite":              "#ffffff",    // Terminal bright white color
    "terminal.ansiBrightYellow":             "#ffcb6b",    // Terminal bright yellow color
    "terminal.ansiCyan":                     "#00a5d1",    // Terminal cyan color
    "terminal.ansiGreen":                    "#0dff00",    // Terminal green color
    "terminal.ansiMagenta":                  "#c792ea",    // Terminal magenta color
    "terminal.ansiRed":                      "#ff3e48",    // Terminal red color
    "terminal.ansiWhite":                    "#ffffff",    // Terminal white color
    "terminal.ansiYellow":                   "#ffcb6b",    // Terminal yellow color
    "terminalCursor.background":             "#000000",    // Terminal cursor background
    "terminalCursor.foreground":             "#ffcb6b",    // Terminal cursor foreground
    "terminal.selectionBackground":          "#2d3540",    // Terminal selection background

    // --- -- - Notifications
    "notifications.background":              "#080808",    // Notification background
    "notifications.foreground":              "#c9d1d9",    // Notification text
    "notifications.border":                  "#2a2e33",    // Notification border
    "notificationCenter.border":             "#2a2e33",    // Notification center border
