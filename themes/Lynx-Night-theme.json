    "terminal.background":                   "#171e21",    // Terminal background
    "terminal.foreground":                   "#c9d1d9",    // Terminal text
    "terminal.ansiBlack":                    "#000000",    // Terminal black color
    "terminal.ansiBlue":                     "#5fc05c",    // Terminal blue color
