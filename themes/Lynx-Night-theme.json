  "tokenColors": [
    {
      "name": "Comment Keywords",
      "scope": [
        "comment.line.double-slash keyword",
        "comment.block keyword"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#6b7280"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSDoc Tags",
      "scope": [
        "storage.type.class.jsdoc",
        "entity.name.type.instance.jsdoc"
      ],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },

    // --- -- - Variables adicionales
    {
      "name": "Variable Destructuring",
      "scope": [
        "variable.other.destructuring"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },

    // --- -- - Propiedades y claves de objeto
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.property.js",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },

    // --- -- - Keywords adicionales
    {
      "name": "Control Flow Keywords",
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import/Export Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "italic"
      }
    },

    // --- -- - Tipos adicionales
    {
      "name": "Type Parameters",
      "scope": [
        "entity.name.type.parameter",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#c78a7a",
        "fontStyle": "italic"
      }
    },

    // --- -- - Strings adicionales
    {
      "name": "Template Strings",
      "scope": [
        "string.template",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "String Escape Characters",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "bold"
      }
    },

    // --- -- - Operadores especiales
    {
      "name": "Special Operators",
      "scope": [
        "keyword.operator.ternary",
        "keyword.operator.spread",
        "keyword.operator.rest"
      ],
      "settings": {
        "foreground": "#F39C12"
      }
    },

    // --- -- - Puntuación adicional
    {
      "name": "Brackets and Parentheses",
      "scope": [
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "punctuation.section.braces",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },

    // --- -- - HTML/XML adicional
    {
      "name": "HTML/XML Attribute Values",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "HTML Doctype",
      "scope": [
        "meta.tag.sgml.doctype",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#6b7280"
      }
    },

    // --- -- - CSS adicional
    {
      "name": "CSS Functions",
      "scope": [
        "support.function.css"
      ],
      "settings": {
        "foreground": "#9B59B6"
      }
    },

    // --- -- - JavaScript/TypeScript específico
    {
      "name": "Decorators",
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "foreground": "#F39C12"
      }
    },
    {
      "name": "Arrow Functions",
      "scope": [
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#F39C12"
      }
    },

    // --- -- - Markdown adicional
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code",
        "punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Lists",
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "markup.list.numbered.markdown",
        "markup.list.unnumbered.markdown"
      ],
      "settings": {
        "foreground": "#F39C12"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote.markdown",
        "punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },

    // --- -- - YAML
    {
      "name": "YAML Keys",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "YAML Values",
      "scope": [
        "string.unquoted.yaml"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },

    // --- -- - Python específico
    {
      "name": "Python Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#F39C12"
      }
    },
    {
      "name": "Python f-strings",
      "scope": [
        "storage.type.string.python"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Python Magic Methods",
      "scope": [
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#9B59B6",
        "fontStyle": "bold"
      }
    },

    // --- -- - PHP específico
    {
      "name": "PHP Variables",
      "scope": [
        "variable.other.php",
        "punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "name": "PHP Tags",
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
