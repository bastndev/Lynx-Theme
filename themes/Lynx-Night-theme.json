    // TODO: -  BG store vscode MARK:[+]
    "textBlockQuote.background":             "#000000",    // Block quote background
    "textBlockQuote.border":                 "#232323",    // Block quote border LEFT
    "textCodeBlock.background":              "#000000",    // Code block background 

    // ---- -- - Selection BG
    "selection.background":                  "#567281",    // selection background

    // --- -- - BG message error
    "inputValidation.errorBackground":       "#E91E6320",  // Error message background
