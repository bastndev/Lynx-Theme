    // TODO: MARK: [PLUS]
    // --- -- - progress bar
    "progressBar.background":                "#13ff3e",    // Progress bar background

    //ctrl+shift+p | Inputs
    "editorWidget.background":               "#161F24",    // Background
    "quickInputList.focusBackground":        "#13ff3e60",  // Focus background
    "list.highlightForeground":              "#5fc05c",    // Highlight text color

    "keybindingLabel.foreground":            "#fefefe",    // Keybinding label text
    "keybindingLabel.background":            "#000000",    // Keybinding label background
    "keybindingLabel.border":                "#08080800",    // Keybinding label border
    "keybindingLabel.bottomBorder":          "#000000",    // Keybinding label bottom border

    // --- -- - Panel Terminal & Text BOTTOM
    "panel.background":                      "#161F24",    // Panel background
    "panel.border":                          "#161F24",    // Panel border
    "panelTitle.activeBorder":               "#8FBC8F",    // Active panel title border
    "panelTitle.activeForeground":           "#c9d1d9",    // Active panel title text
    "panelTitle.inactiveForeground":         "#6b7280",    // Inactive panel title text
