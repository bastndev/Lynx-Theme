  "semanticTokenColors": {
    "namespace": "#c9b35a",
    "property": "#f5d6c8",
    "interface": "#7fb3d3",
    "type": "#7fb3d3",
    "class": "#7fb3d3",
    "enum": "#c9b35a",
    "function": "#97c93d",
    "method": "#97c93d",
    "macro": "#7859bc",
    "variable": "#f5d6c8",
    "variable.readonly": "#b8a082",
    "parameter": "#97c93d",
    "property.readonly": "#b8a082",
    "enumMember": "#97c93d",
    "event": "#d47373"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#8a7562"
      }
    },
    {
      "name": "Operators and Punctuation",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#b8a082"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f5d6c8"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d47373"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ba5a47"
      }
    },
    {
      "name": "Meta Tag",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#f5d6c8"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#a67c52"
      }
    },
    {
      "name": "Css color #number",
      "scope": [
        "punctuation",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Tag",
